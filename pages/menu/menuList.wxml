  <!--使用animation属性指定需要执行的动画-->
  <view animation="{{animationData}}" class="drawer_box" wx:if="{{showModalStatus}}">

    <!--drawer content-->
    <view class="drawer_title">圣罗兰口红</view>
    <view class="drawer_content">
      <!--产品明细 start-->
      <view class='section_item'>
        <image src='https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1521560864960&di=0b8d8a2bfbff0460b62ffa8e0f52a0d5&imgtype=0&src=http%3A%2F%2Fimg.zcool.cn%2Fcommunity%2F011fd959af638ba8012028a9a78b05.jpg%401280w_1l_2o_100sh.jpg' class='image_size'></image>
