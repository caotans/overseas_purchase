          <label>$</label>345</view>
        <view class='total_count'>
          <view>
            <icon type="success" size="10" />数量
            <label>1</label>
            <label>件</label>
          </view>
          <view>
            <icon type="warn" size="10" /> 库存
            <label>30</label>
            <label>片</label>
          </view>


        </view>
        <view>
          <slider block-size='20' block-color='#666666' class='product_slider' min='1' max='20' step='1'></slider>
        </view>
        <view class='total_count'>
          <view>
            <icon type="success" size="10" />拼单数量
